{
  "chain_id": "SN_INTEGRATION_SEPOLIA",
  "image": "ghcr.io/starkware-libs/sequencer/sequencer:dev",
  "application_config_subdir": "config/sequencer/sepolia_integration/app_configs/consolidated/integration_consolidated/",
  "services": [
    {
      "name": "Node",
      "controller": "StatefulSet",
      "config_path": "node.json",
      "ingress": {
        "domain": "sw-dev.io",
        "internal": false,
        "rules": [
          {
            "path": "/gateway",
            "port": 8080,
            "backend": null
          },
          {
            "path": "/feeder_gateway",
            "port": 8085,
            "backend": "nginx-service"
          }
        ],
        "alternative_names": [
          "starknet-upgrade-testing-sepolia.gateway-proxy.sw-dev.io"
        ]
      },
      "autoscale": false,
      "replicas": 1,
      "storage": 500,
      "toleration": "sequencer",
      "resources": {
        "requests": {
          "cpu": 2,
          "memory": 4
        },
        "limits": {
          "cpu": 4,
          "memory": 8
        }
      },
      "external_secret": {
        "gcsm_key": "node-1-integration-secrets"
      }
    }
  ]
}
