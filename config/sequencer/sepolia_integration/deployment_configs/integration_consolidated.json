{
  "chain_id": "SN_INTEGRATION_SEPOLIA",
  "image": "ghcr.io/starkware-libs/sequencer/sequencer:dev",
  "application_config_subdir": "config/sequencer/sepolia_integration/app_configs/consolidated",
  "services": [
    {
      "name": "Node",
      "controller": "StatefulSet",
      "config_path": "node.json",
      "ingress": {
        "domain": "sw-dev.io",
        "internal": true,
        "rules": [
          {
            "path": "/gateway",
            "port": 8080
          },
          {
            "path": "/feeder-gateway",
            "port": 8080
          }
        ],
        "alternative_names": []
      },
      "autoscale": false,
      "replicas": 1,
      "storage": 32,
      "toleration": null,
      "resources": {
        "requests": {
          "cpu": 1,
          "memory": 2
        },
        "limits": {
          "cpu": 4,
          "memory": 8
        }
      },
      "external_secret": {
        "gcsm_key": "sequencer-dev-secrets"
      }
    }
  ]
}
