      "conditions": [
        {
          "evaluator": {
            "params": [
              0.0
            ],
            "type": "gt"
          },
          "operator": {
            "type": "and"
          },
          "reducer": {
            "params": [],
            "type": "avg"
          },
          "type": "query"
        }
      ],
      "for": "30s",
      "intervalSec": 30,
      "severity": "p4"
    },
